<h1>User</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">User name</th>
            <th scope="col">sur name</th>
            <th scope="col">id</th>
            <th scope="col">userPrincipalName</th>
        </tr>
    </thead>
    <tbody>
        {{#each events}}
        <tr>
            <td>{{this.displayName}}</td>
            <td>{{this.surname}}</td>
            <td>{{this.id}}</td>
            <td>{{this.userPrincipalName}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>